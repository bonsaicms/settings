{
    "name": "bonsaicms/settings",
    "type": "package",
    "description": "Settings manager for Laravel",
    "authors": [
        {
            "name": "Matej Vitaz",
            "email": "mspiderv@gmail.com"
        }
    ],
    "keywords": [
        "settings",
        "Bonsai CMS",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0.2",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "BonsaiCms\\Settings\\": "src/BonsaiCms/Settings/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BonsaiCms\\Settings\\SettingsServiceProvider"
            ],
            "aliases": {
                "Settings": "BonsaiCms\\Settings\\SettingsFacade"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.4|^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.3.1"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
